# Environment Variables for MyBuilding App
# Copy this to .env.local for local development
# Set these in Vercel Dashboard > Settings > Environment Variables for production

# Supabase Configuration (get from Supabase Dashboard > Settings > API)
VITE_SUPABASE_URL=https://your-project-id.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Admin Configuration
VITE_ADMIN_EMAIL=your-admin-email@example.com
VITE_ADMIN_EMAILS=
VITE_INVITATION_SECRET=change-this-to-a-random-string

# Building Configuration
VITE_BUILDING_NAME=שם הבניין
VITE_BUILDING_NAME_EN=Building Name
VITE_BUILDING_ADDRESS=כתובת הבניין
VITE_BUILDING_ADDRESS_EN=Building Address
